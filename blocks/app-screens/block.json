{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "mellobase/app-screens",
  "version": "0.1.0",
  "title": "App Screens",
  "category": "design",
  "icon": "smiley",
  "description": "Dynamically display a set of app screens in an iphone mockup",
  "example": {},
  "supports": {
    "html": false,
    "position": {
      "sticky": true
    }
  },
  "attributes": {
    "images": {
      "type": "array",
      "default": [],
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "number"
          },
          "url": {
            "type": "string"
          },
          "alt": {
            "type": "string"
          },
          "key": {
            "type": "string"
          }
        }
      }
    }
  },
  "textdomain": "app-screens",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js"
}