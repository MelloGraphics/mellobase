{
	"version": 3,
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"settings": {
		"appearanceTools": true,
		"color": {
			"defaultPalette": false,
			"defaultGradients": false,
			"defaultDuotone": false,
			"customDuotone": true,
			"custom": true,
			"palette": [
				{
					"slug": "base",
					"color": "#f1f1f1",
					"name": "White"
				},
				{
					"slug": "contrast",
					"color": "#051214",
					"name": "Black"
				},
				{
					"slug": "primary",
					"color": "#F15A24",
					"name": "Orange"
				},
				{
					"slug": "secondary",
					"color": "#101E3B",
					"name": "Navy"
				},
				{
					"slug": "tertiary",
					"color": "#1D1D1D",
					"name": "Grey"
				},
				{
					"slug": "transparent",
					"color": "transparent",
					"name": "Transparent"
				}
			],
			"gradients": [
				{
					"slug": "d-transparent-primary",
					"gradient": "linear-gradient(to bottom right,transparent 49.9%,#b0d29d 50%)",
					"name": "Diagonal transparent to primary"
				},
				{
					"slug": "d-primary-transparent",
					"gradient": "linear-gradient(to bottom right, #b0d29d 49.9%, transparent 50%)",
					"name": "Diagonal primary to transparent"
				}
			]
		},
		"layout": {
			"contentSize": "1000px",
			"wideSize": "1400px"
		},
		"spacing": {
			"units": [ "px", "em", "rem", "svh", "svw", "%" ],
			"defaultSpacingSizes": false,
			"spacingSizes": [
				{
					"size": "0.67rem",
					"slug": "small",
					"name": "S"
				},
				{
					"size": "clamp(.5rem, 1rem, 1.5rem)",
					"slug": "medium",
					"name": "M"
				},
				{
					"size": "1.5rem",
					"slug": "large",
					"name": "L"
				},
				{
					"size": "2.25rem",
					"slug": "x-large",
					"name": "XL"
				},
				{
					"size": "clamp(3.25rem, 10vw, 5rem)",
					"slug": "xx-large",
					"name": "XXL"
				},
				{
					"size": "clamp(4.38rem, 30vmax, 10.06rem)",
					"slug": "section",
					"name": "Section"
				}
			]
		},
		"typography": {
			"defaultFontSizes": false,
			"dropCap": true,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Montserrat",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": "file:./fonts/Montserrat-Bold.ttf"
						},
						{
							"fontFamily": "Montserrat",
							"fontStyle": "italic",
							"fontWeight": "700",
							"src": "file:./fonts/Montserrat-BoldItalic.ttf"
						},
						{
							"fontFamily": "Montserrat",
							"fontStyle": "normal",
							"fontWeight": "500",
							"src": "file:./fonts/Montserrat-Medium.ttf"
						},
						{
							"fontFamily": "Montserrat",
							"fontStyle": "italic",
							"fontWeight": "500",
							"src": "file:./fonts/Montserrat-MediumItalic.ttf"
						},
						{
							"fontFamily": "Montserrat",
							"fontStyle": "normal",
							"fontWeight": "250",
							"src": "file:./fonts/Montserrat-Thin.ttf"
						},
						{
							"fontFamily": "Montserrat",
							"fontStyle": "italic",
							"fontWeight": "250",
							"src": "file:./fonts/Montserrat-ThinItalic.ttf"
						}
					],
					"fontFamily": "Montserrat",
					"name": "Montserrat",
					"slug": "primary"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Fontawesome",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": "file:./fonts/fontawesome/sharp/fa-sharp-regular-400.ttf"
						}
					],
					"fontFamily": "Fontawesome",
					"name": "Fontawesome",
					"slug": "icon"
				}
			],
			"fontSizes": [
				{
					"slug": "small",
					"size": ".8rem",
					"name": "Small",
					"fluid": {
						"min": ".7rem",
						"max": ".9rem"
					}
				},
				{
					"slug": "medium",
					"size": "1rem",
					"name": "Medium",
					"fluid": {
						"min": ".9rem",
						"max": "1.2rem"
					}
				},
				{
					"slug": "large",
					"size": "1.5rem",
					"name": "Large",
					"fluid": {
						"min": "1.1rem",
						"max": "1.7rem"
					}
				},
				{
					"slug": "x-large",
					"size": "2rem",
					"name": "XL",
					"fluid": {
						"min": "1.2rem",
						"max": "2.3rem"
					}
				},
				{
					"slug": "xx-large",
					"size": "3.75rem",
					"name": "XXL",
					"fluid": {
						"min": "2.2rem",
						"max": "5rem"
					}
				}
			]
		},
		"custom": {
			"transition": "cubic-bezier(0, 0, 0, 1.01)",
			"transition-quick": "cubic-bezier(0, 0, 0, 1.01)",
			"mask": "linear-gradient(0deg, rgba(255,255,255,0) 5%, rgba(255,255,255,1) 100%)"
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"css": ".site-footer{ margin-block-start: 0;}",
		"blocks": {
			"core/heading": {
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				"typography": {
					"lineHeight": "1.3"
				}
			},
			"core/comment-author-name": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comment-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comment-edit-link": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comment-reply-link": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/details": {
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--medium)",
						"right": "var(--wp--preset--spacing--0)",
						"bottom": "var(--wp--preset--spacing--medium)",
						"left": "var(--wp--preset--spacing--0)"
					}
				}
			},
			"core/cover": {
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--medium)",
						"right": "var(--wp--preset--spacing--large)",
						"bottom": "var(--wp--preset--spacing--medium)",
						"left": "var(--wp--preset--spacing--large)"
					}
				}
			},
			"core/footnotes": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--extra-small)"
				},
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--contrast)"
						}
					}
				}
			},
			"core/latest-comments": {
				"spacing": {
					"padding": {
						"left": "0px"
					}
				}
			},
			"core/navigation": {
				"elements": {
					"link": {
						"typography": {
						},
						":hover": {
							"typography": {
								"textDecoration": "none"
							}
						}
					}
				}
			},
			"core/page-list": {
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "underline"
						},
						":hover": {
							"typography": {
								"textDecoration": "none"
							}
						}
					}
				}
			},
			"core/separator": {
				"color": {
					"background": "var(--wp--preset--color--contrast)"
				}
			},
			"core/post-author-name": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--extra-small)"
				},
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--primary)"
						}
					}
				}
			},
			"core/post-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--extra-small)"
				},
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--primary)"
						}
					}
				}
			},
			"core/post-navigation-link": {
				"typography": {
					"fontWeight": "700"
				}
			},
			"core/post-terms": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--extra-small)"
				},
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--primary)"
						}
					}
				}
			},
			"core/post-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"lineHeight": "1.3"
				}
			},
			"core/query-pagination": {
				"typography": {
					"fontWeight": "700"
				},
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--xx-large)",
						"bottom": "var(--wp--preset--spacing--xx-large)"
					}
				}
			},
			"core/query-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"core/search": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/site-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				},
				"elements": {
					"link":{
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			},
			"core/quote": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontStyle": "italic"
				},
				"elements": {
					"link":{
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"spacing": {
					"margin": {
						"top": "var(--wp--preset--spacing--0)",
						"right": "var(--wp--preset--spacing--0)",
						"bottom": "var(--wp--preset--spacing--0)",
						"left": "var(--wp--preset--spacing--0)"
					}
				}
			},
			"woocommerce/product-categories": {
				"css": "& ul{list-style:none;} & ul:first-child{padding-left:0;}"
			},
			"woocommerce/product-price": {
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				"elements": {
					"link":{
						"color": {
							"text": "var(--wp--preset--color--contrast)"
						}
					}
				}
			},
			"woocommerce/filter-wrapper": {
				"elements": {
					"heading": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--medium)",
							"fontWeight": "400"
						}
					}
				}
			},
			"yoast/faq-block": {
				"css" : "& .schema-faq-section { background: var(--wp--preset--color--contrast); color: var(--wp--preset--color--contrast); padding: 8px 4px 8px 32px; margin: 4px 0;} & .schema-faq-answer{line-height: inherit; margin: 0;}"
			}
		},
		"color": {
			"background": "var(--wp--preset--color--base)",
			"text": "var(--wp--preset--color--contrast)"
		},
		"elements": {
			"link": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--primary)"
					},
					"typography": {
						"textDecoration": "none"
					}
				},
				":focus": {
					"color": {
						"text": "var(--wp--preset--color--primary)"
					}
				},
				":active": {
					"color": {
						"text": "var(--wp--preset--color--primary)"
					}
				}
			},
			"button": {
				"border": {
					"style": "none",
					"radius": "0"
				},
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--base)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				},
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--medium)",
						"right": "var(--wp--preset--spacing--medium)",
						"bottom": "var(--wp--preset--spacing--medium)",
						"left": "var(--wp--preset--spacing--medium)"
					}
				},
				":hover": {
				}
			}
		},
		"typography": {
			"fontSize": "var(--wp--preset--font-size--medium)",
			"fontFamily": "var(--wp--preset--font-family--primary)",
			"lineHeight": "1.5"
		},
		"spacing":{
			"blockGap": "var(--wp--preset--spacing--large)",
			"padding": {
				"top": "0",
				"right": "var(--wp--preset--spacing--large)",
				"bottom": "var(--wp--preset--spacing--large)",
				"left": "var(--wp--preset--spacing--large)"
			}
		}
	},
	"templateParts": [
		{
		  "name": "header",
		  "title": "Header",
		  "area": "header"
		},
		{
		  "name": "footer",
		  "title": "Footer",
		  "area": "footer"
		}
	],
	"customTemplates": [
		{
			"name": "blank",
			"title": "Blank page",
			"postTypes": [ "page" ]
		}
	]
}
