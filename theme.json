{
	"version": 3,
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"settings": {
		"appearanceTools": true,
		"color": {
			"defaultPalette": false,
			"defaultGradients": false,
			"defaultDuotone": false,
			"customDuotone": true,
			"custom": true,
			"palette": [
				{
					"slug": "base",
					"color": "#ffffff",
					"name": "Base"
				},
				{
					"slug": "contrast",
					"color": "#09002E",
					"name": "Navy"
				},
				{
					"slug": "primary",
					"color": "#EE255A",
					"name": "Red"
				},
				{
					"slug": "secondary",
					"color": "#A37FCC",
					"name": "Purple"
				},
				{
					"slug": "tertiary",
					"color": "#E9DFF2",
					"name": "Purple 75"
				},
				{
					"slug": "tertiary-2",
					"color": "#F4EFF9",
					"name": "Purple 50"
				},
				{
					"slug": "tertiary-3",
					"color": "#F8F4FB",
					"name": "Purple 25"
				},
				{
					"slug": "transparent",
					"color": "transparent",
					"name": "Transparent"
				}
			],
			"gradients": [
				{
					"slug": "d-transparent-primary",
					"gradient": "linear-gradient(to bottom right,transparent 49.9%,#b0d29d 50%)",
					"name": "Diagonal transparent to primary"
				},
				{
					"slug": "d-primary-transparent",
					"gradient": "linear-gradient(to bottom right, #b0d29d 49.9%, transparent 50%)",
					"name": "Diagonal primary to transparent"
				}
			]
		},
		"layout": {
			"contentSize": "900px",
			"wideSize": "1400px"
		},
		"spacing": {
			"units": [ "px", "em", "rem", "svh", "svw", "%" ],
			"defaultSpacingSizes": false,
			"spacingSizes": [
				{
					"size": "0.67rem",
					"slug": "small",
					"name": "S"
				},
				{
					"size": "clamp(.5rem, 1rem, 1.5rem)",
					"slug": "medium",
					"name": "M"
				},
				{
					"size": "1.5rem",
					"slug": "large",
					"name": "L"
				},
				{
					"size": "2.25rem",
					"slug": "x-large",
					"name": "XL"
				},
				{
					"size": "clamp(3.25rem, 10vw, 5rem)",
					"slug": "xx-large",
					"name": "XXL"
				},
				{
					"size": "clamp(4.38rem, 30vmax, 7rem)",
					"slug": "section",
					"name": "Section"
				}
			]
		},
		"typography": {
			"defaultFontSizes": false,
			"dropCap": true,
			"fluid": true,
			"writingMode": true,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "soehne",
							"fontStyle": "normal",
							"fontWeight": "800",
							"src": "file:./fonts/soehne/woff2/soehne-fett.woff2"
						},
						{
							"fontFamily": "soehne",
							"fontStyle": "italic",
							"fontWeight": "800",
							"src": "file:./fonts/soehne/woff2/soehne-fett-kursiv.woff2"
						},
						{
							"fontFamily": "soehne",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": "file:./fonts/soehne/woff2/soehne-halbfett.woff2"
						},
						{
							"fontFamily": "soehne",
							"fontStyle": "italic",
							"fontWeight": "700",
							"src": "file:./fonts/soehne/woff2/soehne-halbfett-kursiv.woff2"
						},
						{
							"fontFamily": "soehne",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": "file:./fonts/soehne/woff2/soehne-buch.woff2"
						},
						{
							"fontFamily": "soehne",
							"fontStyle": "italic",
							"fontWeight": "400",
							"src": "file:./fonts/soehne/woff2/soehne-buch-kursiv.woff2"
						}
					],
					"fontFamily": "soehne",
					"name": "soehne",
					"slug": "primary"
				}
			],
			"fontSizes": [
				{
					"slug": "small",
					"size": ".75rem",
					"name": "Small",
					"fluid": {
						"min": ".7rem",
						"max": ".8rem"
					}
				},
				{
					"slug": "medium",
					"size": ".8rem",
					"name": "Medium",
					"fluid": {
						"min": ".8rem",
						"max": "1rem"
					}
				},
				{
					"slug": "large",
					"size": "1rem",
					"name": "Large",
					"fluid": {
						"min": ".9rem",
						"max": "1.2rem"
					}
				},
				{
					"slug": "x-large",
					"size": "1.7rem",
					"name": "XL",
					"fluid": {
						"min": "1.1rem",
						"max": "1.8rem"
					}
				},
				{
					"slug": "xx-large",
					"size": "2rem",
					"name": "XXL",
					"fluid": {
						"min": "1rem",
						"max": "3.8rem"
					}
				}
			]
		},
		"custom": {
			"ease-bounce": "linear(0, 0.49 7.4%, 0.864 15.3%, 1.005 19.4%, 1.12 23.7%, 1.206 28.1%, 1.267 32.8%, 1.296 36.4%, 1.311 40.2%, 1.313 44.2%, 1.301 48.6%, 1.252 56.9%, 1.105 74.4%, 1.048 82.5%, 1.011 91.1%, 1)",
			"ease-circ": "cubic-bezier(0.075, 0.820, 0.165, 1.000)",
			"speed-fast": ".25s",
			"speed-medium": ".5s",
			"speed-slow": "1s",
			"radius-small": "5px",
			"radius-medium": "10px",
			"radius-large": "20px"
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"css": ".site-footer{ margin-block-start: 0;}",
		"blocks": {
			"core/heading": {
				"typography": {
					"lineHeight": "1.1",
					"fontWeight": "800",
					"fontStyle": "italic"
				}
			},
			"core/comment-author-name": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comment-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comment-edit-link": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comment-reply-link": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/cover": {
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--medium)",
						"right": "var(--wp--preset--spacing--large)",
						"bottom": "var(--wp--preset--spacing--medium)",
						"left": "var(--wp--preset--spacing--large)"
					}
				}
			},
			"core/footnotes": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--extra-small)"
				},
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--contrast)"
						}
					}
				}
			},
			"core/latest-comments": {
				"spacing": {
					"padding": {
						"left": "0px"
					}
				}
			},
			"core/navigation": {
				"elements": {
					"link": {
						"typography": {
						},
						":hover": {
							"typography": {
								"textDecoration": "none"
							}
						}
					}
				}
			},
			"core/page-list": {
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "underline"
						},
						":hover": {
							"typography": {
								"textDecoration": "none"
							}
						}
					}
				}
			},
			"core/separator": {
				"color": {
					"background": "var(--wp--preset--color--contrast)"
				}
			},
			"core/post-author-name": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--extra-small)"
				},
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--primary)"
						}
					}
				}
			},
			"core/post-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--extra-small)"
				},
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--primary)"
						}
					}
				}
			},
			"core/post-navigation-link": {
				"typography": {
					"fontWeight": "700"
				}
			},
			"core/post-terms": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--extra-small)"
				},
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--primary)"
						}
					}
				}
			},
			"core/post-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"lineHeight": "1.1"
				}
			},
			"core/query-pagination": {
				"typography": {
					"fontWeight": "700"
				},
				"spacing": {
					"margin": {
						"top": "var(--wp--preset--spacing--xx-large)",
						"bottom": "var(--wp--preset--spacing--xx-large)"
					}
				}
			},
			"core/query-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"core/site-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				},
				"elements": {
					"link":{
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			},
			"core/quote": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontStyle": "italic"
				},
				"elements": {
					"link":{
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"spacing": {
					"margin": {
						"top": "var(--wp--preset--spacing--0)",
						"right": "var(--wp--preset--spacing--0)",
						"bottom": "var(--wp--preset--spacing--0)",
						"left": "var(--wp--preset--spacing--0)"
					}
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--base)",
			"text": "var(--wp--preset--color--contrast)"
		},
		"elements": {
			"link": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--primary)"
					},
					"typography": {
						"textDecoration": "none"
					}
				},
				":focus": {
					"color": {
						"text": "var(--wp--preset--color--primary)"
					}
				},
				":active": {
					"color": {
						"text": "var(--wp--preset--color--primary)"
					}
				}
			},
			"button": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "700"
				},
				"border": {
					"radius": "var(--wp--custom--radius-small)",
					"style": "none"
				},
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--small)",
						"right": "var(--wp--preset--spacing--medium)",
						"bottom": "var(--wp--preset--spacing--small)",
						"left": "var(--wp--preset--spacing--medium)"
					}
				},
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--base)"
				},
				":hover": {
				}
			}
		},
		"typography": {
			"fontSize": "var(--wp--preset--font-size--medium)",
			"fontFamily": "var(--wp--preset--font-family--primary)",
			"lineHeight": "1.5"
		},
		"spacing":{
			"blockGap": "var(--wp--preset--spacing--large)",
			"padding": {
				"top": "0",
				"right": "clamp(2rem, 6vmin, 10rem)",
				"bottom": "var(--wp--preset--spacing--large)",
				"left": "clamp(2rem, 6vmin, 10rem)"
			}
		}

	},
	"templateParts": [
		{
			"name": "header",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "footer",
			"title": "Footer",
			"area": "footer"
		}
	],
	"customTemplates": [
		{
			"name": "blank",
			"title": "Blank page",
			"postTypes": [ "page" ]
		},
		{
			"name": "archive-work",
			"title": "Archive: Work",
			"postTypes": [ "work" ]
		},
		{
			"name": "single-work",
			"title": "Single Item: Work",
			"postTypes": [ "work" ]
		},
		{
			"name": "archive-tests",
			"title": "Archive: Tests",
			"postTypes": [ "work" ]
		},
		{
			"name": "single-tests",
			"title": "Single Item: Test",
			"postTypes": [ "tests" ]
		},
		{
			"name": "archive-services",
			"title": "Archive: Services",
			"postTypes": [ "services" ]
		},
		{
			"name": "single-services",
			"title": "Single Item: Service",
			"postTypes": [ "services" ]
		},
		{
			"name": "archive-solutions",
			"title": "Archive: Solutions",
			"postTypes": [ "solutions" ]
		},
		{
			"name": "single-solutions",
			"title": "Single Item: Solution",
			"postTypes": [ "solutions" ]
		},
		{
			"name": "archive-team",
			"title": "Archive: Team",
			"postTypes": [ "team-members" ]
		},
		{
			"name": "single-team",
			"title": "Single Item: Team",
			"postTypes": [ "team" ]
		},
		{
			"name": "archive-glossary",
			"title": "Archive: Glossary",
			"postTypes": [ "glossary" ]
		},
		{
			"name": "single-glossary",
			"title": "Single Item: Glossary",
			"postTypes": [ "glossary" ]
		},
		{
			"name": "archive-faq",
			"title": "Archive: FAQ",
			"postTypes": [ "faq" ]
		},
		{
			"name": "single-faq",
			"title": "Single Item: FAQ",
			"postTypes": [ "faq" ]
		},
		{
			"name": "single-portfolio",
			"title": "Single Item: Portfolio",
			"postTypes": [ "portfolio" ]
		}
	]
}