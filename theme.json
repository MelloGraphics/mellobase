{
	"version": 3,
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"settings": {
		"appearanceTools": true,
		"color": {
			"defaultPalette": false,
			"defaultGradients": false,
			"defaultDuotone": false,
			"customDuotone": true,
			"custom": true,
			"palette": [
				{
					"slug": "primary",
					"color": "#1c5dd7",
					"name": "Primary"
				},
				{
					"slug": "primary-contrast",
					"color": "#ffffff",
					"name": "Primary Conrast"
				},
				{
					"slug": "contrast",
					"color": "#141414",
					"name": "Contrast"
				},
				{
					"slug": "secondary",
					"color": "#0e12442",
					"name": "Secondary"
				},
				{
					"slug": "tertiary-1",
					"color": "#a0abc0",
					"name": "Tertiary 1"
				},
				{
					"slug": "tertiary-2",
					"color": "#cbd2e0",
					"name": "Tertiary 2"
				},
				{
					"slug": "tertiary-3",
					"color": "#1D1D1D",
					"name": "Tertiary 3"
				},

				{
					"slug": "base",
					"color": "#f1f1f1",
					"name": "Base"
				},
				{
					"slug": "transparent",
					"color": "transparent",
					"name": "Transparent"
				}
			],
			"gradients": [
				{
					"slug": "d-transparent-primary",
					"gradient": "linear-gradient(to bottom right,transparent 49.9%,#b0d29d 50%)",
					"name": "Diagonal transparent to primary"
				},
				{
					"slug": "d-primary-transparent",
					"gradient": "linear-gradient(to bottom right, #b0d29d 49.9%, transparent 50%)",
					"name": "Diagonal primary to transparent"
				}
			]
		},
		"layout": {
			"contentSize": "1000px",
			"wideSize": "1400px"
		},
		"spacing": {
			"units": [ "px", "em", "rem", "svh", "svw", "%" ],
			"defaultSpacingSizes": false,
			"spacingSizes": [
				{
					"size": "clamp(0.125rem,  0.651vw, 0.3125rem)",
					"slug": "x-small",
					"name": "XS"
				},
				{
					"size": "clamp(0.3125rem, 1.302vw, 0.9375rem)",
					"slug": "small",
					"name": "S"
				},
				{
					"size": "clamp(1.25rem,   3.255vw, 1.875rem)",
					"slug": "medium",
					"name": "M"
				},
				{
					"size": "clamp(3.125rem,  7.81vw, 4.375rem)",
					"slug": "large",
					"name": "L"
				},
				{
					"size": "clamp(3.75rem,  10.41vw, 5.625rem)",
					"slug": "x-large",
					"name": "XL"
				},
				{
					"size": "clamp(4.0625rem, 11.72vw, 9.375rem)",
					"slug": "xx-large",
					"name": "XXL"
				},
				{
					"size": "clamp(4.375rem, 13.02vw, 11.25rem)",
					"slug": "section",
					"name": "Section"
				}
			]
		},
		"typography": {
			"defaultFontSizes": false,
			"dropCap": true,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Montserrat",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": "file:./assets/fonts/montserrat/Montserrat-Bold.ttf"
						},
						{
							"fontFamily": "Montserrat",
							"fontStyle": "italic",
							"fontWeight": "700",
							"src": "file:./assets/fonts/montserrat/Montserrat-BoldItalic.ttf"
						},
						{
							"fontFamily": "Montserrat",
							"fontStyle": "normal",
							"fontWeight": "500",
							"src": "file:./assets/fonts/montserrat/Montserrat-Medium.ttf"
						},
						{
							"fontFamily": "Montserrat",
							"fontStyle": "italic",
							"fontWeight": "500",
							"src": "file:./assets/fonts/montserrat/Montserrat-MediumItalic.ttf"
						},
						{
							"fontFamily": "Montserrat",
							"fontStyle": "normal",
							"fontWeight": "250",
							"src": "file:./assets/fonts/montserrat/Montserrat-Thin.ttf"
						},
						{
							"fontFamily": "Montserrat",
							"fontStyle": "italic",
							"fontWeight": "250",
							"src": "file:./assets/fonts/montserrat/Montserrat-ThinItalic.ttf"
						}
					],
					"fontFamily": "Montserrat",
					"name": "Montserrat",
					"slug": "primary"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Fontawesome",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": "file:./assets/fonts/fontawesome/sharp/fa-sharp-regular-400.ttf"
						}
					],
					"fontFamily": "Fontawesome",
					"name": "Fontawesome",
					"slug": "icon"
				}
			],
			"fontSizes": [
				{
					"slug": "small",
					"size": ".75rem",
					"name": "Small",
					"fluid": {
						"min": ".7rem",
						"max": ".8rem"
					}
				},
				{
					"slug": "medium",
					"size": "1rem",
					"name": "Medium",
					"fluid": {
						"min": ".9rem",
						"max": "1rem"
					}
				},
				{
					"slug": "large",
					"size": "1.25rem",
					"name": "Large",
					"fluid": {
						"min": "1rem",
						"max": "1.25rem"
					}
				},
				{
					"slug": "x-large",
					"size": "1.5rem",
					"name": "XL",
					"fluid": {
						"min": "1.4rem",
						"max": "1.6rem"
					}
				},
				{
					"slug": "xx-large",
					"size": "2.125rem",
					"name": "XXL",
					"fluid": {
						"min": "1.875rem",
						"max": "2.5rem"
					}
				}
			]
		},
		"custom": {
			"ease-bounce": "linear(0, 0.49 7.4%, 0.864 15.3%, 1.005 19.4%, 1.12 23.7%, 1.206 28.1%, 1.267 32.8%, 1.296 36.4%, 1.311 40.2%, 1.313 44.2%, 1.301 48.6%, 1.252 56.9%, 1.105 74.4%, 1.048 82.5%, 1.011 91.1%, 1)",
			"ease-circ": "cubic-bezier(0.075, 0.820, 0.165, 1.000)",
			"speed-fast": ".25s",
			"speed-medium": ".5s",
			"speed-slow": "1s"
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/heading": {
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				"typography": {
					"lineHeight": "1.3"
				}
			},
			"core/comment-author-name": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comment-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comment-edit-link": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comment-reply-link": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/details": {
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				}
			},
			"core/cover": {
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--medium)",
						"right": "var(--wp--preset--spacing--large)",
						"bottom": "var(--wp--preset--spacing--medium)",
						"left": "var(--wp--preset--spacing--large)"
					}
				}
			},
			"core/footnotes": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--extra-small)"
				},
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--contrast)"
						}
					}
				}
			},
			"core/latest-comments": {
				"spacing": {
					"padding": {
						"left": "0px"
					}
				}
			},
			"core/navigation": {
				"elements": {
					"link": {
						"typography": {
						},
						":hover": {
							"typography": {
								"textDecoration": "none"
							}
						}
					}
				}
			},
			"core/page-list": {
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "underline"
						},
						":hover": {
							"typography": {
								"textDecoration": "none"
							}
						}
					}
				}
			},
			"core/separator": {
				"color": {
					"background": "var(--wp--preset--color--contrast)"
				}
			},
			"core/post-author-name": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--extra-small)"
				},
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--primary)"
						}
					}
				}
			},
			"core/post-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--extra-small)"
				},
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--primary)"
						}
					}
				}
			},
			"core/post-navigation-link": {
				"typography": {
					"fontWeight": "700"
				}
			},
			"core/post-terms": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--extra-small)"
				},
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--primary)"
						}
					}
				}
			},
			"core/post-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"lineHeight": "1.3"
				}
			},
			"core/query-pagination": {
				"typography": {
					"fontWeight": "700"
				},
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--xx-large)",
						"bottom": "var(--wp--preset--spacing--xx-large)"
					}
				}
			},
			"core/query-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"core/search": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/site-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				},
				"elements": {
					"link":{
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			},
			"core/quote": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontStyle": "italic"
				},
				"elements": {
					"link":{
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"spacing": {
					"margin": {
						"top": "var(--wp--preset--spacing--0)",
						"right": "var(--wp--preset--spacing--0)",
						"bottom": "var(--wp--preset--spacing--0)",
						"left": "var(--wp--preset--spacing--0)"
					}
				}
			},
			"woocommerce/product-categories": {
				"css": "& ul{list-style:none;} & ul:first-child{padding-left:0;}"
			},
			"woocommerce/product-price": {
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				"elements": {
					"link":{
						"color": {
							"text": "var(--wp--preset--color--contrast)"
						}
					}
				}
			},
			"woocommerce/filter-wrapper": {
				"elements": {
					"heading": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--medium)",
							"fontWeight": "400"
						}
					}
				}
			},
			"yoast/faq-block": {
				"css" : "& .schema-faq-section { background: var(--wp--preset--color--contrast); color: var(--wp--preset--color--contrast); padding: 8px 4px 8px 32px; margin: 4px 0;} & .schema-faq-answer{line-height: inherit; margin: 0;}"
			}
		},
		"color": {
			"background": "var(--wp--preset--color--base)",
			"text": "var(--wp--preset--color--contrast)"
		},
		"elements": {
			"link": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--primary)"
					},
					"typography": {
						"textDecoration": "none"
					}
				},
				":focus": {
					"color": {
						"text": "var(--wp--preset--color--primary)"
					}
				},
				":active": {
					"color": {
						"text": "var(--wp--preset--color--primary)"
					}
				}
			},
			"button": {
				"border": {
					"style": "none",
					"radius": "0"
				},
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--primary-contrast)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				},
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--medium)",
						"right": "var(--wp--preset--spacing--medium)",
						"bottom": "var(--wp--preset--spacing--medium)",
						"left": "var(--wp--preset--spacing--medium)"
					}
				},
				":hover": {
				}
			}
		},
		"typography": {
			"fontSize": "var(--wp--preset--font-size--medium)",
			"fontFamily": "var(--wp--preset--font-family--primary)",
			"lineHeight": "1.5"
		},
		"spacing":{
			"blockGap": "var(--wp--preset--spacing--medium)",
			"padding": {
				"top": "0",
				"right": "var(--wp--preset--spacing--large)",
				"bottom": "var(--wp--preset--spacing--large)",
				"left": "var(--wp--preset--spacing--large)"
			}
		}
	},
	"templateParts": [
		{
		  "name": "header",
		  "title": "Header",
		  "area": "header"
		},
		{
		  "name": "footer",
		  "title": "Footer",
		  "area": "footer"
		}
	],
	"customTemplates": [
		{
			"name": "blank",
			"title": "Blank page",
			"postTypes": [ "page" ]
		},
		{
			"name": "archive-tests",
			"title": "Archive: Tests",
			"postTypes": [ "work" ]
		},
		{
			"name": "single-tests",
			"title": "Single Item: Test",
			"postTypes": [ "tests" ]
		}
	]
}
